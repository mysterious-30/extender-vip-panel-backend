{
  "name": "extender-vip-panel-backend",
  "version": "0.1.0",
  "description": "Backend for Extender VIP Panel",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "nodemon --ignore 'node_modules/' server.js",
    "start": "node server.js",
    "build": "tsc",
    "tsc": "tsc",
    "vercel-build": "npm run build",
    "vercel-start": "node server.js",
    "logs": "node scripts/view-logs.js",
    "logs:errors": "node scripts/view-logs.js --level=error",
    "logs:mongodb": "node scripts/view-logs.js --mongodb",
    "logs:client": "node scripts/view-logs.js --source=client",
    "deploy:check": "node scripts/deploy.js",
    "deploy": "npm run deploy:check && npm run build"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "firebase": "^10.9.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.2.3",
    "mongodb": "^6.15.0",
    "next": "^13.5.4",
    "next-auth": "^4.24.6",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "zod": "^3.22.4",
    "@upstash/redis": "^1.28.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/mongodb": "^4.0.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.17.31",
    "@types/react": "^18.2.66",
    "nodemon": "^3.0.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
